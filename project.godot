; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="American Football"
run/main_scene="res://Game.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GlobalData="*res://GlobalData.tscn"

[display]

window/size/viewport_height=690

[editor_plugins]

enabled=PackedStringArray("res://addons/jam_launch/editor_plugin/plugin.cfg")

[physics]

2d/default_gravity_vector=Vector2(0, 0)

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.270588, 0.156863, 0.235294, 1)
